    <owl:Class rdf:about="https://purl.archive.org/sopontology#Document">
        <rdfs:subClassOf rdf:resource="https://purl.archive.org/sopontology#SopItem"/>
        <rdfs:comment xml:lang="pt">Representa documentos auxiliares associados a um Procedimento Operacional Padrão, como formulários, registros ou anexos.</rdfs:comment>
        <rdfs:comment xml:lang="en">Represents auxiliary documents associated with a Standard Operating Procedure, such as forms, records, or attachments.</rdfs:comment>
        <rdfs:label xml:lang="en">Document</rdfs:label>
        <rdfs:label xml:lang="pt">Documento</rdfs:label>
    </owl:Class>
    



    <!-- https://purl.archive.org/sopontology#Activity -->

    <owl:Class rdf:about="https://purl.archive.org/sopontology#Activity">
        <rdfs:subClassOf rdf:resource="https://purl.archive.org/sopontology#Entity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.archive.org/sopontology#performedAt"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.archive.org/sopontology#performedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Generic superclass that aggregates common descriptive properties shared by all entities in the ontology, such as name, description, and multimedia references.</rdfs:comment>
        <rdfs:comment xml:lang="pt">Superclasse genérica que agrega propriedades descritivas comuns a todas as entidades da ontologia, como nome, descrição e referências multimídia.</rdfs:comment>
        <rdfs:label xml:lang="pt">Entidade</rdfs:label>
        <rdfs:label xml:lang="en">Entity</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.archive.org/sopontology#BibliographicReference -->

    <owl:Class rdf:about="https://purl.archive.org/sopontology#BibliographicReference">
        <rdfs:subClassOf rdf:resource="https://purl.archive.org/sopontology#SopItem"/>
        <rdfs:comment xml:lang="pt">Representa referências bibliográficas ou normativas que fundamentam ou regulam um Procedimento Operacional Padrão.</rdfs:comment>
        <rdfs:comment xml:lang="en">Represents bibliographic or normative references that support or regulate a Standard Operating Procedure.</rdfs:comment>
        <rdfs:label xml:lang="en">Bibliographic Reference</rdfs:label>
        <rdfs:label xml:lang="pt">Referência Bibliográfica</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.archive.org/sopontology#BooleanExpression -->

    <owl:Class rdf:about="https://purl.archive.org/sopontology#BooleanExpression">
        <rdfs:subClassOf rdf:resource="https://purl.archive.org/sopontology#Entity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="https://purl.archive.org/sopontology#guardCondition"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="https://purl.archive.org/sopontology#posCondition"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="https://purl.archive.org/sopontology#preCondition"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.archive.org/sopontology#name"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="pt">Representa uma expressão lógica utilizada para definir pré-condições, pós-condições ou condições de guarda que controlam o fluxo de execução de atividades e transições.</rdfs:comment>
        <rdfs:comment xml:lang="en">Represents a logical expression used to define preconditions, postconditions, or guard conditions controlling the execution flow of activities and transitions.</rdfs:comment>
        <rdfs:label xml:lang="en">Boolean Expression</rdfs:label>
        <rdfs:label xml:lang="pt">Expressão Booleana</rdfs:label>
    </owl:Class>
    



    </owl:Class>
    


    <!-- https://purl.archive.org/sopontology#OrganizationPerformer -->

    <owl:Class rdf:about="https://purl.archive.org/sopontology#OrganizationPerformer">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/org#Organization"/>
        <rdfs:subClassOf rdf:resource="https://purl.archive.org/sopontology#Performer"/>
        <rdfs:comment xml:lang="pt">Representa um executor definido como uma organização responsável pela execução de atividades dentro de um procedimento.</rdfs:comment>
        <rdfs:comment xml:lang="en">Represents a performer defined as an organization responsible for executing activities within a procedure.</rdfs:comment>
        <rdfs:label xml:lang="pt">Executor por Organização</rdfs:label>
        <rdfs:label xml:lang="en">Organization-based Performer</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.archive.org/sopontology#OrganizationPlace -->

    <owl:Class rdf:about="https://purl.archive.org/sopontology#OrganizationPlace">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/org#Organization"/>
